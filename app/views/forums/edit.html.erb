<h2>Edit Forum</h2>
<%= error_messages_for :forum %>
<% form_for(:forum,
             :url => forum_path(@forum),
             :html => {:method => :put}) do |f| %>
<%= render :partial => 'form', :locals => {:f => f} -%>
<%= submit_tag "Update" %>
<% end %>
<%= link_to 'Show', forum_path(@forum) %> |
<%= link_to 'Back', forum_path %>
